<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <div class="card">
        <div class="card-header">
          My Tableau Dashboard
        </div>
        <div class="card-body">
          <div class="tableauPlaceholder" id="myViz"></div>
          <script src="https://public.tableau.com/javascripts/api/tableau-2.min.js"></script>
          <script>
            var placeholderDiv = document.getElementById("myViz");
            var url = "https://public.tableau.com/views/MyTableauDashboard/MyDashboard?:embed=y&:display_count=yes";
            var options = {
              width: "100%",
              height: "800px",
              hideTabs: true,
              hideToolbar: true,
              onFirstInteractive: function () {
                console.log("Tableau dashboard loaded successfully");
              }
            };
            var viz = new tableau.Viz(placeholderDiv, url, options);
          </script>
        </div>
      </div>
    </div>
  </div>
</div>
